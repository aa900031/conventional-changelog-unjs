		"defu": "^6.1.4",
		"scule": "^1.3.0"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "^3.1.0",
		"@aa900031/release-it-config": "^0.1.0",
		"@vitest/coverage-istanbul": "^3.2.4",
		"conventional-changelog-writer": "^8.2.0",
		"eslint": "^9.32.0",
		"release-it": "^19.0.4",
